	<title>James's Projects</title>

	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
	<link rel="stylesheet" type="text/css" href="/css/JamesPortfolio.css">
	<script src="https://code.jquery.com/jquery-3.3.1.min.js"
  			integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
  			crossorigin="anonymous"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous">
  	</script>
  	

</head>

<body>

	<nav class="navbar navbar-inverse ">
		<div class="container">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-nav-demo" aria-expanded="false">
					<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
        			<span class="icon-bar"></span>
        			<span class="icon-bar"></span>
				</button>
				<a href="http://localhost:3000/Info_James" class="navbar-brand"><span class="glyphicon glyphicon-user" aria-hidden="true"></span> My Profile</a>
			</div>

			<div class="collapse navbar-collapse" id="bs-nav-demo">
				<ul class="nav navbar-nav">

						<li class="dropdown">

							<a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Team Members<span class="caret"></span>
							</a>

							<ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
								<li><a href="#">Liam O'Connor</a></li>
								<li><a href="#">Belanna Marconi</a></li>
							</ul>

						</li>

				</ul>

				<ul class="nav navbar-nav navbar-right">
					<!-- <li><a href="#">Admin Login</a></li> -->
					<li><a href="http://localhost:3000/">Team Page</a></li>
				</ul>
			</div>	
		</div>	
	</nav>

	<div class="container" id="projects_wrap">
	<div id="projects">
	<!-- /////////// JS Carousel /////////// -->
		

		<div id="myCarousel" class="carousel slide" data-ride="carousel">
		<!-- Indicators -->
			<ol class="carousel-indicators">
				<li data-target="#myCarousel" data-slide-to="0" class="active"></li>
				<% for (i = 1; i < projects.length; i++) { %>
					<li data-target="#myCarousel" data-slide-to="<%= i %>"></li>
				<% } %>
			</ol>

			  <!-- Wrapper for slides -->
			<div class="carousel-inner" role="listbox">
				
				
				<% for (i = 0; i < projects.length; i++) { 
					if (i === 0) { %>
					<div class="item active"> <% } 
					else { %>
					<div class="item"> <% } %>
						<img src="<%= projects[i].image %>" alt="Project">
						<div class="carousel-caption">
							<h3><%= projects[i].name %></h3>
							<p><%= projects[i].description %></p>
						</div>
						<div id="projectLink" class="projectLink">
							<a class="btn btn-primary" href="http://localhost:3000/Projects_James/<%= projects[i].link%>" role="button"><span class="glyphicon glyphicon-new-window"></span> View Project </a>
						</div>
					</div>

					
				<% } %>

			</div>

			<!-- Left and right controls -->
			<a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
				<span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
				<span class="sr-only">Previous</span>
			</a>
			<a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
				<span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
				<span class="sr-only">Next</span>
			</a>


		</div>
	</div>
	</div>

</body>

<script type="text/javascript" src="/js/JamesPortfolio.js"></script>

</html>









